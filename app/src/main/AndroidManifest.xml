<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.justine.auxilium">

    <application
        android:name=".AuxiliumApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.justine.auxilium.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.justine.auxilium.activities.AddCarActivity" />
        <activity android:name="com.justine.auxilium.activities.AddLandActivity" />
        <activity android:name="com.justine.auxilium.activities.DetailCarActivity" />
        <activity android:name="com.justine.auxilium.activities.SearchLandActivity" />
        <activity android:name="com.justine.auxilium.activities.DetailLandActivity" />
        <activity android:name="com.justine.auxilium.activities.SearchCarActivity" />
        <activity android:name="com.justine.auxilium.activities.SearchItemActivity" />
        <activity android:name="com.justine.auxilium.activities.AddItemActivity" />

    </application>

</manifest>